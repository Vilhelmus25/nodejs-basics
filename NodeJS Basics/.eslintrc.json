{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "standard"
    ],
    "plugins": [
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "no-multi-spaces": "off",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "eol-last": "off",
        "func-call-spacing": "off",
        "no-unexpected-multiline": "off",
        "spaced-comment": "off"
    }
}